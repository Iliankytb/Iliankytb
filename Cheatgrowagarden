	})
local ACP = GameTab:AddToggle({
		Text = "Auto Collects Fruits",
		Name = "AFCP",
		Flag = "AFCP",--Put a name flag for save data if you enabled saving data
		Default = false,
		Callback = function(state)
AutoCollectFruits = state
task.spawn(function()
while AutoCollectFruits do
if AutoCollectFruits and not InSellingInventory and not InSelling then
for _, farm in pairs(workspace.Farm:GetChildren()) do 
if farm.Important.Data.Owner.Value == game.Players.LocalPlayer.Name then 
for _, part in pairs(farm.Important.Plants_Physical:GetChildren()) do 
if part:isA("Model") then
if part:FindFirstChild("Fruits") then 
for _, fruits in pairs(part:FindFirstChild("Fruits"):GetChildren()) do 
if fruits.PrimaryPart and  AutoCollectFruits and not InSellingInventory and not InSelling then
game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = fruits.PrimaryPart.CFrame 
for _, base in pairs(fruits:GetChildren()) do 
if base:isA("BasePart") and base:FindFirstChildOfClass("ProximityPrompt") then
local prompt = base:FindFirstChildOfClass("ProximityPrompt")
prompt:InputHoldBegin()
task.wait(prompt.HoldDuration)
wait(0.15)
end
end  
end  
end 
end 
end 
end  
end
end
end
wait(0.1)
end
game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
end)
		end
	})
	local TextBoxT1 = GameTab:AddTextbox({
		Text = "",
		Name = "TextBoxT1",
		Placeholder = "Put the name of a tier 1 seed",
		Flag = "TextboxT1",--Put a name flag for save data if you enabled saving data
		Callback = function(text)
CurrentItemSelectedT1 = text
		end
	})
