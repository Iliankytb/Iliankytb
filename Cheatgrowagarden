		Text = "Discord Link",
		Name = "ButtonDCL",
		Callback = function() 	
			library:CopyText("https://discord.gg/E2TqYRsRP4")
		end
	})
local SellInventory = GameTab:AddButton({
		Text = "Sell Inventory",
		Name = "ButtonSI",
		Callback = function() 	
AutoSellInventory = true
wait(0.25)
Game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(
86.5898285, 2.99999976, 0.426797628, -0.0030667223, 5.64698901e-08, -0.999995291, -5.71349608e-13, 1, 5.64701601e-08, 0.999995291, 1.7374964e-10, -0.0030667223)
wait(0.1)
game:GetService("ReplicatedStorage"):WaitForChild("GameEvents"):WaitForChild("Sell_Inventory"):FireServer()
wait(0.5)
AutoSellInventory = false
end
	})
local SellItem = GameTab:AddButton({
		Text = "Sell Current Item",
		Name = "ButtonSCI",
		Callback = function() 	
InSelling = true
wait(0.25)
Game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(
86.5898285, 2.99999976, 0.426797628, -0.0030667223, 5.64698901e-08, -0.999995291, -5.71349608e-13, 1, 5.64701601e-08, 0.999995291, 1.7374964e-10, -0.0030667223)
