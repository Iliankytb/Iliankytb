local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/shlexware/Orion/main/source"))()
local Window = OrionLib:MakeWindow({Name = "Cheat Evade", HidePremium = false,IntroEnabled = false ,SaveConfig = true, ConfigFolder = "OrionTest"})
local HumanoidRootPart = game.Player.LocalPlayer.Character.HumanoidRootPart
local CandyName = {
"Ticket1"
}

_G.AutoFarmCandysNoSecurity = true
_G.AutoFarmCandysSecurity = true

local function AutoFarmCandysNoSecurity()
for _, Candys in ipairs(Workspace:GetChildren())
HumanoidRootPart.CFrame = Candys.CFrame
Wait(.0000000001)
end
end

local MainTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local SectionMain = MainTab:AddSection({
	Name = "Auto Farm"
})

local SectionHalloweenevent = MainTab:AddSection({
	Name = "Halloween Event"
})

MainTab:AddToggle({
	Name = "Auto Farm Candy(with no security)",
	Default = false,
	Callback = function(Value)
		_G.AutoFarmNoSecurity = Value
	end    
})

Workspace.DecendantsAdded:Connect(function()
while _G.AutoFarmNoSecurity == true do
end
end)

OrionLib:Init()
