library:LoadData()
task.spawn(function()
while true do
	task.wait(1) 
task.spawn(function()
	local updatedInfo = getServerInfo()
	local updatedContent = string.format(
		"ðŸ“Œ PlaceId: %s\nðŸ”‘ JobId: %s\nðŸ§ª IsStudio: %s\nðŸ‘¥ Players: %d/%d",
