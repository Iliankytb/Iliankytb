    Name = "How Aim",
    Options = {"Camera", "Character"},
    CurrentOption = {"Camera"},
    MultipleOptions = false,
    Flag = "AimbotDropdown",
    Callback = function(Options)
        if Options[1] == "Camera" then
            HowAim = "Camera"
        elseif Options[1] == "Character" then
            HowAim = "Character"
        end
    end,
})

