HowManyItemCanShowUp = HowManyItemCanShowUp +1
isInTheMap ="yes"
end 
end
Label:Set("Item Is In The Map: "..isInTheMap.." (x"..HowManyItemCanShowUp..")","rewind")
   end,
})
