   Flag = "NoFog1", 
   Callback = function(Value)
ActiveNoFog = Value 
if ActiveNoFog then
for _, part in pairs(Workspace.Map.Boundaries:GetChildren()) do 
	if part:isA("Part") then
		part:Destroy()
	end
end  
end
end,
})
