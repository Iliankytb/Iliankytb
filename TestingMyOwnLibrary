local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Iliankytb/Iliankytb/main/IliankLybrary"))()

library:CreateWindow({
	Title = "Title of script",-- Title of the script
	Icon = false,-- Icon or no (use rbxAssets://) (coming soon)
	Intro = false,--Intro or no
	IntroTitle = "Intro Title",--the intro title
	Data = {
		EnableSavingData = true,--Enabling data saver or no
FolderName = "YAYYYfinally",
		FileName = "TestingNow",-- File name for the data saver
	},
Discord = {
Enabled = false,--enable to copy the discord
DiscordLink = "NoInviteLink",-- put thr entire Link of discord
RememberJoin = true,-- Set this ti false to make them copy the link every time they load the script
},
	KeySystem = false,--Key System or no
	KeySettings = {
		Title = "key System Title",--Title of the key
		Note = "No methode of obtaining",-- The method for obtaining
		FileName = "Key",-- File name or no for saving the key (coming soon)
		SaveKey = true,-- saving the key or no
		Key = {"Key"},-- the name (you can add many name, Exemple "Key","Key2"
	},
}, function(window)
	local tab = window:CreateTab("Tab 1")-- Tabs

--Button--
--Text--
	local firsttext = tab:AddText({
		Text = "This is a Text",
		Name = "Text"
	})

	tab:AddButton({
		Text = "This is a button",
		Name = "Button",
		Callback = function() 
		firsttext:Set("okay")	
		end
	})
--Toggle--
	tab:AddToggle({
		Text = "This is a Toggle",
		Name = "Toggle",
		Flag = "okay",--Put a name flag for save data if you enabled saving data
		Default = false,
		Callback = function(state)
			
		end
	})

--Color Picker--
	tab:AddColorPicker({
		Text = "This is a color picker",
		Name = "Color picker",
		Color = Color3.new(255, 255, 255),
		Flag = "like",--Put a name flag for save data if you enabled saving data
		Callback = function(val)
		
		end
	})



--Paragraph--
	tab:AddParagraph({
		Title = "This is the title of a paragraph",
	Content = "This is a paragraph"	
	})

--Slider--
	tab:AddSlider({
		Name = "This is a Slider",
		Min = 0,
		Max = 100,
		Default = 0,
		Flag = "Okay2",--Put a name flag for save data if you enabled saving data
		Callback = function(val)
			
		end
	})

--Textbox--
	tab:AddTextbox({
		Text = "This is a textbox",
		Name = "TextBox",
		Placeholder = "This is a TextBox",
		Flag = "YAY",--Put a name flag for save data if you enabled saving data
		Callback = function(text)
			
		end
	})

--Dropdown--
	tab:AddDropdown({
		Text = "This is a Dropdown",
		Name = "Dropdown",
		Options = {"Option 1", "Option 2", "Option 3"},
		Default = "This is a Dropdown",
		MultiSelect = true,
Flag = "Droping", --Put a name flag for save data if you enabled saving data
		Callback = function(choice)
			
		end
	})

	--notif--
	window:Notify({
		Title = "Testing!",
		Message = "notif!",
		Duration = 5,
	})

--Loading Data--
	library:LoadData()
end)
