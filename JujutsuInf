local V = "Version: 0.0.5"
print(V)
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Jujutsu Inf Cheat "..V, HidePremium = false,IntroEnabled = false ,SaveConfig = false, ConfigFolder = "Tester"})

local AutoMissions = false

local function AutoMissionsFarm()
while AutoMissions do
-- Script generated by TurtleSpy, made by Intrer#0421

game:GetService("ReplicatedStorage").Remotes.Server.Data.TakeQuest:InvokeServer({["type"] = "Band",["set"] = "Shijo Town Set",["rewards"] = {["essence"] = 9999,["chestMeter"] = 100,["exp"] = 9999999,["cash"] = 999999},["rewardsText"] = "$999999 | 999999 EXP | 99999 Mission Essence",["difficulty"] = 2,["title"] = "Defeat a band",["amount"] = 3,["level"] = 413,["grade"] = "Special Grade",["subtitle"] = "of 3 curse users"})
game:GetService("ReplicatedStorage").Remotes.Server.Combat.M1:FireServer(1,{})
game:GetService("ReplicatedStorage").Remotes.Server.Combat.ChangeWeapon:FireServer("Fists")
wait(0.01)
end
end

local HubAutoFarmTab = Window:MakeTab({
	Name = "Auto Farm",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


HubAutoFarmTab:AddToggle({
	Name = "Auto Missions",
	Default = false,
	Callback = function(Value)
		AutoMissions = Value
AutoMissionsFarm()
	end    
})

OrionLib:Init()
