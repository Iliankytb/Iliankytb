local V = "Version: 0.0.34"
print(V)
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Jujutsu Inf Cheat "..V, HidePremium = false,IntroEnabled = false ,SaveConfig = false, ConfigFolder = "Tester"})
local AutoMissions = false
local BlocksPlayers = false
local BlocksMobs = false
local godmode = false
local AlwaysBF = false
local AutoAuraKill = false
local NoCooldownValue = false
local Player = game.Players.LocalPlayer
local SkillsFolder = game:GetService("ReplicatedStorage").Skills
local UseAllPotion = false
local UseAllGourd = false
local UseAllCat = false
local UseAllFood = false
local SktDivesAlrUsed = false
local ContextActionService = game:GetService("ContextActionService")
local redeemedFolders = Player.redeemed

local function AutoMissionsFarm()
game:GetService("ReplicatedStorage").Remotes.Server.Combat.ChangeWeapon:FireServer("Fists")
while AutoMissions do
game:GetService("ReplicatedStorage").Remotes.Server.Data.TakeQuest:InvokeServer({["subtitle"] = "a Curse User",["set"] = "Yuki Fortress Set",["rewards"] = {["essence"] = 40,["chestMeter"] = 79,["exp"] = 8850000,["cash"] = 51000},["rewardsText"] = "8.85 Mi EXP | ILIANKYTB The best",["difficulty"] = 3,["title"] = "Defeat",["level"] = 430,["grade"] = "Non Sorcerer",["type"] = "Kill"})
--game:GetService("ReplicatedStorage").Remotes.Server.Dialogue.GetResponse:InvokeServer("Clan Head Jujutsu High","Promote")
game:GetService("ReplicatedStorage").Remotes.Server.Data.ChestMeterReward:FireServer("Yuki Fortress Set")
local PlayersFolder = workspace.Objects.Mobs
local Hum = Player.Character:WaitForChild("HumanoidRootPart")

for _, playerModel in pairs(PlayersFolder:GetChildren()) do
    if playerModel:FindFirstChild("Humanoid") and playerModel:FindFirstChild("HumanoidRootPart") then
        local targetRootPart = playerModel.HumanoidRootPart
        local distance = (Hum.Position - targetRootPart.Position).Magnitude
      if distance >= 1000 and not SktDivesAlrUsed then
SktDivesAlrUsed = true
wait(1)
end
if distance <= 250 then
Hum.CFrame = targetRootPart.CFrame
end
if distance <= 25 then
        
            game:GetService("ReplicatedStorage").Remotes.Server.Combat.M1:FireServer(4, {playerModel.Humanoid})
 if playerModel.Humanoid.Health == 0 and  SktDivesAlrUsed then
SktDivesAlrUsed = false
end
        end
    end
end

wait(0.001)
end
end

local function NoCooldown()
while NoCooldownValue do
for _, SkillsFolder2 in pairs(SkillsFolder:GetChildren()) do
SkillsFolder2.Cooldown.Value = 0
end
Player.Character:WaitForChild("Humanoid").CombatAgent.Multipliers.Cooldown.Value = 0.1
wait(0.0000001)
end
end

local function AutoUseAllPotions()
while UseAllPotion do
game:GetService("ReplicatedStorage").Remotes.Server.Data.EquipItem:InvokeServer("Luck Vial")
game:GetService("ReplicatedStorage").Remotes.Server.Data.EquipItem:InvokeServer("Health Vial")
game:GetService("ReplicatedStorage").Remotes.Server.Data.EquipItem:InvokeServer("Damage Vial")
game:GetService("ReplicatedStorage").Remotes.Server.Data.EquipItem:InvokeServer("Focus Vial")
game:GetService("ReplicatedStorage").Remotes.Server.Data.EquipItem:InvokeServer("Agility Vial")
wait(1)
end
end

local function AutoUseAllGourd()
while UseAllGourd do
game:GetService("ReplicatedStorage").Remotes.Server.Data.EquipItem:InvokeServer("Focus Gourd")
game:GetService("ReplicatedStorage").Remotes.Server.Data.EquipItem:InvokeServer("Damage Gourd")
game:GetService("ReplicatedStorage").Remotes.Server.Data.EquipItem:InvokeServer("Rage Gourd")
wait(1)
end
end

local function AutoUseAllCat()
while UseAllCat do
game:GetService("ReplicatedStorage").Remotes.Server.Data.EquipItem:InvokeServer("Withered Beckoning Cat")
wait(300)
end
end

local function AutoUseAllFood()
while UseAllFood do
game:GetService("ReplicatedStorage").Remotes.Server.Data.EquipItem:InvokeServer("Dorayaki")
game:GetService("ReplicatedStorage").Remotes.Server.Data.EquipItem:InvokeServer("Takoyaki")
wait(60)
end
end

local function AlwaysBFFunction()
while AlwaysBF do
game:GetService("ReplicatedStorage").Remotes.Server.Combat.ApplyBlackFlashToNextHitbox:FireServer()
wait(0.01)
end
end

local function AuraKill()
while AutoAuraKill do
local PlayersFolder = workspace.Objects.Mobs
local Hum = Player.Character:WaitForChild("HumanoidRootPart")

for _, playerModel in pairs(PlayersFolder:GetChildren()) do
    if playerModel:FindFirstChild("Humanoid") and playerModel:FindFirstChild("HumanoidRootPart") then
        local targetRootPart = playerModel.HumanoidRootPart
        local distance = (Hum.Position - targetRootPart.Position).Magnitude
        if distance <= 250 then
Hum.CFrame = targetRootPart.CFrame
end
if distance <= 25 then
        
            game:GetService("ReplicatedStorage").Remotes.Server.Combat.M1:FireServer(4, {playerModel.Humanoid})
 
        end
    end
end

Player.Character:WaitForChild("Humanoid").CombatAgent.Multipliers.Hitbox.Value = 50
wait(0.001)
end
end

local function RedeemAllCodes()
for _, redeemedFolders2 in pairs(redeemedFolders:GetChildren()) do
game:GetService("ReplicatedStorage").Remotes.Server.Data.RedeemCode:InvokeServer(redeemedFolders2.Name)
end
end

local function BlocksAllPlayers()
while BlocksPlayers do
    local PlayersFolder = workspace.Objects.Characters

    for _, playerModel in pairs(PlayersFolder:GetChildren()) do
game:GetService("ReplicatedStorage").Remotes.Server.Combat.Rush:FireServer(playerModel.Humanoid,false)
    end
wait(0.0000000000001)
end
end

local function BlocksAllMobs()
while BlocksMobs do
    local PlayersFolder = workspace.Objects.Mobs
    for _, playerModel in pairs(PlayersFolder:GetChildren()) do
game:GetService("ReplicatedStorage").Remotes.Server.Combat.Rush:FireServer(playerModel.Humanoid,false)
    end
wait(0.000000000001)
end
end

local HubAutoFarmTab = Window:MakeTab({
	Name = "Auto Farm",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local HubItemsTab = Window:MakeTab({
	Name = "Items",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

HubItemsTab:AddToggle({
	Name = "Auto Use All Potions(Every Second)",
	Default = false,
	Callback = function(Value)
		UseAllPotion = Value
AutoUseAllPotions()
	end    
})

HubItemsTab:AddToggle({
	Name = "Auto Use All Foods(Every 60 Seconds)",
	Default = false,
	Callback = function(Value)
		UseAllFood = Value
AutoUseAllFood()
	end    
})

HubItemsTab:AddToggle({
	Name = "Auto Use All Cat(Every 5 Minute,300 Seconds)",
	Default = false,
	Callback = function(Value)
		UseAllCat = Value
AutoUseAllCat()
	end    
})

HubItemsTab:AddToggle({
	Name = "Auto Use All Gourd(Every Second)",
	Default = false,
	Callback = function(Value)
		UseAllGourd = Value
AutoUseAllGourd()
	end    
})

HubAutoFarmTab:AddToggle({
	Name = "Auto Missions",
	Default = false,
	Callback = function(Value)
		AutoMissions = Value
AutoMissionsFarm()
	end    
})

HubAutoFarmTab:AddToggle({
	Name = "Block All Players",
	Default = false,
	Callback = function(Value)
		BlocksPlayers = Value
BlocksAllPlayers()
	end    
})

HubAutoFarmTab:AddToggle({
	Name = "Full God Mode",
	Default = false,
	Callback = function(Value)
		godmode = Value
if godmode then
game:GetService("ReplicatedStorage").Remotes.Server.Combat.Skill:FireServer("Domain Expansion: Idle Death Gamble")
wait(3)
game:GetService("ReplicatedStorage").Remotes.Server.Combat.ToggleMenu:FireServer(true)
wait()
game:GetService("ReplicatedStorage").Remotes.Server.Combat.Skydive:FireServer(true)
end
	end    
})

HubAutoFarmTab:AddToggle({
	Name = "Block All Mobs",
	Default = false,
	Callback = function(Value)
		BlocksMobs = Value
BlocksAllMobs()
	end    
})

HubAutoFarmTab:AddToggle({
	Name = "Aura Kill",
	Default = false,
	Callback = function(Value)
		AutoAuraKill = Value
AuraKill()
	end    
})

HubAutoFarmTab:AddToggle({
	Name = "No Cooldown Skills",
	Default = false,
	Callback = function(Value)
		NoCooldownValue = Value
NoCooldown()
	end    
})


HubAutoFarmTab:AddToggle({
	Name = "Always Black Flash",
	Default = false,
	Callback = function(Value)
		AlwaysBF = Value
AlwaysBFFunction()
	end    
})

HubAutoFarmTab:AddToggle({
	Name = "Redeem All Code",
	Default = false,
	Callback = function(Value)		
RedeemAllCodes()
	end    
})


OrionLib:Init()
