	Name = "Esp Candy",
	Default = false,
	Callback = function(Value)
	
	end    
})


HubEspTab:AddToggle({
	Name = "Esp Items",
	Default = false,
	Callback = function(Value)
	toggleESP(Value,"Items")
	end    
})


HubEspTab:AddToggle({
	Name = "Esp Door",
	Default = false,
