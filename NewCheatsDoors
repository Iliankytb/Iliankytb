	Name = "Esp Monsters",
	Default = false,
	Callback = function(Value)
	toggleESP(Value,"Monster")
	end    
})

HubEspTab:AddToggle({
	Name = "Esp Wardrobe",
	Default = false,
	Callback = function(Value)
	
