local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Iliankytb/Iliankytb/main/Zentrix"))()
      
library:CreateWindow({
    Title = "Fisch by Iliankytb",-- Title of the script
    Theme = "Default",--View more theme in my dicord server!
    Icon = 0,-- 0 = no icon or add number
    Intro = false,--Intro or no
    IntroTitle = "Zentrix Library Testing",--the intro title
    KeyPC = Enum.KeyCode.K,--the key for pc for Open/close the frame
    Data = {
        EnableSavingData = true,--Enabling data saver or no
        DisableNotifyingLoadedData = false,--set it to true for disable the notify when loaded data
        FolderName = "SaverFisch",--Folder Name,you can change it
        FileName = "Iliankytb",-- File name for the data saver,you can change it
    },
    Discord = {
    Enabled = false,--enable to copy the discord
        DiscordLink = "https://discord.gg/E2TqYRsRP4",-- put the entire Link of discord
        RememberJoin = true,-- Set this ti false to make them copy the link every time they load the script
        Duration = 10,-- 5 is the default you can change the number only for remember join
    },
    KeySystem = false,--Key System or no
    KeySettings = {
        Title = "Zentrix Library Testing Key System",--Title of the key(just like that)
        Note = "the key is Zentrix",-- The method for obtaining
        FileName = "Key",-- File name or no for saving the key
        SaveKey = true,-- saving the key or no
        GrabKeyFromSite = false,--Coming soon
        Key = {"Zentrix"},-- the name you can add many name, Exemple "Key","Key2" or you can add a function to add key but i don't really know
        AddGetKeyButton = false,
        AddDiscordButton = true,
        DiscordLink = "NoInviteLink",
        GetKeyLink = "NoKeyLink",
    },
}, function(window)
    local MainTab = window:CreateTab("Main",0)-- Tabs , 0 = no icon or add number
    local TPTab = window:CreateTab("Teleport",0)-- Tabs , 0 = no icon or add number
    local MiscTab = window:CreateTab("MiscTab",0)-- Tabs , 0 = no icon or add number
    local DiscordTab = window:CreateTab("Discord",0)-- Tabs , 0 = no icon or add number
     local SettingsTab = window:CreateTab("Settings",0)-- Tabs , 0 = no icon or add number
     local CanSpawnBoat = true
    local AutoFishValue = false
    local AutoSellEquippedFish = false
    local AutoSellAll = false
    local AutoShake = false
    local AutoCast = false
    local InfOxygen = false
    local AntiAfk = false
    local AutoCastSelectedZone = false
      local CastChoosed = ""
      local SelectedBoat = "Rowboat"
      
    game:GetService("RunService").RenderStepped:Connect(function()
    if AutoFishValue then
    if game.Players.LocalPlayer.PlayerGui:FindFirstChild("reel") then
game.Players.LocalPlayer.PlayerGui.reel.bar.fish.Size = UDim2.new(99,0,1,0)
end
    end
  if AutoSellEquippedFish then
     game:GetService("ReplicatedStorage").events.Sell:InvokeServer()
     end
     if AutoSellAll then
       game:GetService("ReplicatedStorage").events.SellAll:InvokeServer()
     end
     if AutoShake then
     if     game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("shakeui") then
     game:GetService("Players").LocalPlayer.PlayerGui.shakeui.safezone.button.shake:FireServer()
    end
     end
     if AntiAfk then
     game:GetService("ReplicatedStorage").events.afk:FireServer(false)
     end
     if   game.Players.LocalPlayer.Character then
     if AutoCast then
     if  game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool") then
     game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").events.castAsync:InvokeServer(100,100)
     end
     end
if InfOxygen then
game.Players.LocalPlayer.Character.Resources.oxygen.Enabled = false
end
end
if AutoCastSelectedZone then
task.spawn(function()
if not workspace.active.boats:FindFirstChild(game.Players.LocalPlayer.Name) then
if CanSpawnBoat then
CanSpawnBoat = false
workspace.world.npcs[game.Players.LocalPlayer:GetAttribute("LastIsland").." Shipwright"].shipwright.giveUI:InvokeServer()
game:GetService("ReplicatedStorage").packages.Net["RF/Boats/Spawn"]:InvokeServer(SelectedBoat)
wait(1)
CanSpawnBoat = true
game:GetService("Players").LocalPlayer.PlayerGui.hud.safezone.shipwright.Visible = false
end
end
end)
if  workspace.active.boats:FindFirstChild(game.Players.LocalPlayer.Name) then
  if workspace.active.boats:FindFirstChild(game.Players.LocalPlayer.Name):FindFirstChildOfClass("Model") then
if CastChoosed == "" then
        return end
        for _, IslandName in pairs(workspace.zones.fishing:GetChildren()) do
     if IslandName and IslandName:IsA("BasePart") and CastChoosed == IslandName.Name then
    workspace.active.boats:FindFirstChild(game.Players.LocalPlayer.Name):FindFirstChildOfClass("Model").HitBox.Anchored = true
    workspace.active.boats:FindFirstChild(game.Players.LocalPlayer.Name):FindFirstChildOfClass("Model").HitBox.CFrame = IslandName.CFrame + Vector3.new(0,20,0)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.active.boats:FindFirstChild(game.Players.LocalPlayer.Name):FindFirstChildOfClass("Model").HitBox.CFrame
    workspace.active.boats:FindFirstChild(game.Players.LocalPlayer.Name):FindFirstChildOfClass("Model").HitBox.Massless = false
        workspace.active.boats:FindFirstChild(game.Players.LocalPlayer.Name):FindFirstChildOfClass("Model").PlanePart.Anchored = false
    end
     end
      end
      end
end
    end)
    local IslandTP = {}
    local IslandZoneCast = {}
    
    for _, IslandName in pairs(workspace.zones.player:GetChildren()) do
    if IslandName and IslandName:IsA("BasePart") and not table.find(IslandTP,IslandName.Name) then
    table.insert(IslandTP,IslandName.Name)
    end
    end
        for _, IslandName in pairs(workspace.zones.fishing:GetChildren()) do
        if IslandName and IslandName:IsA("BasePart") and not table.find(IslandZoneCast,IslandName.Name) then
    table.insert(IslandZoneCast,IslandName.Name)
    end
    end
    local ButonDiscordLink = DiscordTab:AddButton({
        Text = "Discord Link",
        Name = "Button",
        Callback = function()   
            library:CopyText("https://discord.gg/E2TqYRsRP4")
        end
    })
    --Toggle--
    local AutoFish = MainTab:AddToggle({
    Text = "Auto Fish",
        Name = "AutoFish",
        Flag = "AutoFish",--Put a name flag for save data if you enabled saving data
        Default = false,
        Callback = function(state)
        AutoFishValue = state
        if not AutoFishValue then
        if game.Players.LocalPlayer.PlayerGui:FindFirstChild("reel") then
        game.Players.LocalPlayer.PlayerGui.reel.bar.fish.Size = UDim2.new(0.01,0,2,0)
end
        end
        end
        })
         --Toggle--
         local AutoShake1 = MainTab:AddToggle({
         Text = "Auto Shake",
         Name = "AutoShake1",
         Flag = "AutoShake1",--Put a name flag for save data if you enabled saving data
        Default = false,
        Callback = function(state)
        AutoShake = state
        end
        })
              --Toggle--
              local AutoCast1 = MainTab:AddToggle({
              Text = "Auto Cast",
        Name = "AutoCast1",
        Flag = "AutoCast1",--Put a name flag for save data if you enabled saving data
        Default = false,
        Callback = function(state)
        AutoCast = state
        end
        })
        
        local SellAll = MainTab:AddButton({
        Text = "Sell All",
        Name = "SellAll",
        Callback = function()
        game:GetService("ReplicatedStorage").events.SellAll:InvokeServer()
        end
    })

    local SellEquippedFish = MainTab:AddButton({
    Text = "Sell Equipped Fish",
        Name = "SellEquippedFish",
        Callback = function()
        game:GetService("ReplicatedStorage").events.Sell:InvokeServer()
      end
    })
 --Toggle--
 local AutoSellAll = MainTab:AddToggle({
 Text = "Auto Sell All",
        Name = "AutoSellAll",
        Flag = "AutoSellAll",--Put a name flag for save data if you enabled saving data
        Default = false,
        Callback = function(state)
        AutoSellAll = state
        end
        })
        --Toggle--
        local AutoSellEquippedFish = MainTab:AddToggle({
        Text = "Auto Sell Equipped Fish",
        Name = "AutoSellEquippedFish",
        Flag = "AutoSellEquippedFish",--Put a name flag for save data if you enabled saving data
        Default = false,
        Callback = function(state)
        AutoSellEquippedFish = state
        end
        })
   --Toggle--
   local InfOxy1 = MainTab:AddToggle({
   Text = "Infinite Oxygen",
   Name = "InfOxy1",
   Flag = "InfOxy1",--Put a name flag for save data if you enabled saving data
        Default = false,
        Callback = function(state)
        InfOxygen = state
        if InfOxygen then
        game.Players.LocalPlayer.Character.Resources.oxygen.Enabled = false
        else
        game.Players.LocalPlayer.Character.Resources.oxygen.Enabled = true
        end
        end
        })
          --Toggle--
          local AntiAfk1 = MainTab:AddToggle({
          Text = "Anti Afk",
   Name = "AntiAfk1",
   Flag = "AntiAfk1",--Put a name flag for save data if you enabled saving data
        Default = false,
        Callback = function(state)
        AntiAfk = state
        end
        })
        
    local ChangeThemeDropdown = SettingsTab:AddDropdown({
        Text = "Change Theme:",
        Name = "Dropdown2",
        Options = {"Default", "Light", "RedDark","BlueNeon","GreenMatrix","PurpleDream","Sunset",
        "Ocean","Toxic","Storm","LimePop","Cyber","Chocolate","RoseGold","CottonCandy","NightSky","Steel",
            "GoldLux","Ice","TealWave","FrostFire","DarkMatter","Lava","PinkCandy","Sakura","DeepSpace","Skyline","Crimson"
            ,"Desert","Camo","Plasma","Arctic","Slate","Vaporwave"
            
        },
        Default = "Default",
        MultiSelect = false,
        Flag = "Dropdown2", --Put a name flag for save data if you enabled saving data
        Callback = function(choice)
window:ChangeTheme(choice)
        end
    })
    local IslandChoosed = ""
    local TPDropdown = TPTab:AddDropdown({
    Text = "Teleport to Island:",
        Name = "TPDropdown",
        Options = IslandTP,
        Default = "",
        MultiSelect = false,
        Flag = "TPDropdown", --Put a name flag for save data if you enabled saving data
        Callback = function(choice)
        IslandChoosed = choice
        end
    })
    
    local TeleportToSelectedIsland = TPTab:AddButton({
    Text = "Teleport To Selected Island",
        Name = "TeleportToSelectedIsland",
        Callback = function()
        if IslandChoosed == "" then
        return end
     for _, IslandName in pairs(workspace.zones.player:GetChildren()) do
     if IslandName and IslandName:IsA("BasePart") and IslandChoosed == IslandName.Name then
     game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = IslandName.CFrame
    end
      end
      end
    })
    
    local CastDropdown = MiscTab:AddDropdown({
    Text = "Teleport to Island:",
        Name = "CastDropdown",
        Options = IslandZoneCast,
        Default = "",
        MultiSelect = false,
        Flag = "CastDropdown", --Put a name flag for save data if you enabled saving data
        Callback = function(choice)
        CastChoosed = choice
        end
    })
            --Toggle--
            local AutoCastBoat = MiscTab:AddToggle({
            Text = "Auto Cast Selected Zone",
   Name = "AutoCastBoat",
   Flag = "AutoCastBoat",--Put a name flag for save data if you enabled saving data
        Default = false,
        Callback = function(state)
        AutoCastSelectedZone = state
        if not AutoCastSelectedZone then
        if  workspace.active.boats:FindFirstChild(game.Players.LocalPlayer.Name) then
  if workspace.active.boats:FindFirstChild(game.Players.LocalPlayer.Name):FindFirstChildOfClass("Model") then
  workspace.active.boats:FindFirstChild(game.Players.LocalPlayer.Name):FindFirstChildOfClass("Model").HitBox.Anchored = false
  end
  end
        end
        end
        })
        
   
    --Loading Data--
    library:LoadData()--Add this if you want to load data and everytime at bottom of your code
    end)
