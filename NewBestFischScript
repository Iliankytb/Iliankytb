    task.spawn(function()
    local updatedInfo = getServerInfo()
    local updatedContent = string.format(
        "ðŸŽ® Game: %s\nðŸ“Œ PlaceId: %s\nðŸ”‘ JobId: %s\nðŸ§ª IsStudio: %s\nðŸ‘¥ Players: %d/%d",
